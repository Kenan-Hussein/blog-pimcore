<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    {% if pimcore_head_title().rawContent == '' %}
        {% do pimcore_head_title(document.title) %}
    {% endif %}
    {{ pimcore_head_title() }}
    {% do pimcore_head_meta(document.description, 'description') %}
    {{ pimcore_head_meta() }}

    <title>Tokyo Blog</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
    <link href="{{ asset('assets/s/css/blog.css') }}" rel="stylesheet">
    {#    <link rel="stylesheet" href="{{ asset('assets/dist/css/home-blog.css') }}">#}
</head>

<body>
{%  include 'includes/header.html.twig' %}

{{ block('content') }}

{% if (document.getProperty('footer') is instanceof('\\Pimcore\\Model\\Document\\Snippet')) %}
    {{ pimcore_inc(document.getProperty('footer')) }}
{% endif %}

<script src="{{ asset('assets/dist/js/bootstrap.bundle.min.js') }}"></script>
<script src="{{ asset('assets/js/color-modes.js') }}"></script>
</body>

</html>